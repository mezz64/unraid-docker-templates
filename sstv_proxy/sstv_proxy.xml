<?xml version="1.0"?>
<Container version="2">
  <Name>sstvproxy</Name>
  <Repository>hcgonzalezpr/sstvproxy</Repository>
  <Registry>https://hub.docker.com/r/hcgonzalezpr/sstvproxy</Registry>
  <Network>host</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/vorghahn/sstvProxy</Project>
  <Overview>
    Yet Another Proxy (YAP) for SmoothStreams.tv Docker Image
  </Overview>
  <Category/>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/mezz64/unraid-docker-templates/master/sstv_proxy/sstv_proxy.xml</TemplateURL>
  <Icon/>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Description>
    Fill in the YAP_USERNAME and YAP_PASSWORD variables with your information. [br]
    Choose a YAP_SERVICE, options: view247, vaders, viewss, viewstvn, viewmmasr [br]
    Choose a YAP_SERVER, EG. dnae, deu, dap, etc.
  </Description>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>8098</HostPort>
        <ContainerPort>8098</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8099</HostPort>
        <ContainerPort>8099</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/sstvproxy</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>YAP_USERNAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>YAP_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>YAP_SERVICE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>YAP_SERVER</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>1</Value>
      <Name>YAP_QUALITY</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
</Container>
